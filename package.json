{
  "name": "claimsagent",
  "version": "1.0.0",
  "description": "Healthcare Claims IDP System with LLM-powered extraction and adjudication",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "healthcare",
    "claims",
    "idp",
    "document-processing",
    "ocr",
    "llm"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "dotenv": "^16.3.1",
    "crypto-js": "^4.2.0",
    "express": "^4.21.1",
    "ioredis": "^5.4.1",
    "multer": "^1.4.5-lts.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "sharp": "^0.33.5",
    "tesseract.js": "^5.1.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.9",
    "@types/pdf-parse": "^1.1.4",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
