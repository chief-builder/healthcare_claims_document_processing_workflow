# ClaimsAgent Environment Configuration

# Server
PORT=3000
HOST=0.0.0.0
NODE_ENV=development

# Claude Agent SDK Authentication
# Generate token with: claude setup-token (requires Claude Pro/Max subscription)
CLAUDE_CODE_OAUTH_TOKEN=your-oauth-token-here

# Storage
STORAGE_PATH=./data/storage
UPLOAD_PATH=./data/uploads

# Redis (optional - for queue)
REDIS_URL=redis://localhost:6379

# Processing
MAX_CORRECTION_ATTEMPTS=3
AUTO_PROCESS_CONFIDENCE_THRESHOLD=0.95
CORRECTION_CONFIDENCE_THRESHOLD=0.80

# Logging
LOG_LEVEL=info
LOG_FORMAT=json
